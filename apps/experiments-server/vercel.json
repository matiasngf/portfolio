{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "buildCommand": "cd ../.. && pnpm run experiments:build && cd apps/experiments-server",
  "outputDirectory": "api",
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node",
      "config": { "includeFiles": ["api/**"] }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.js"
    }
  ]
}
